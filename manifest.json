{
  "manifest_version": 2,
  "name": "Crypto Boyz",
  "description": "A simple extension to keep track of crypto prices",
  "version": "1.0",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "index.html",
    "default_title": "Crypto Boyz"
  },
  "permissions": ["activeTab"]
}
